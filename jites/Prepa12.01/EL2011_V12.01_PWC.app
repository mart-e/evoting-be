	from pc_tot A, base_commune B 
	where A.com_ins = b.com_ins and B.Type = 'GE' and (pct_identif like '%0%' or pct_identif is null) and pct_statut like 'Ecr%' " || lsTemp ||"
	order by 7,3,4
