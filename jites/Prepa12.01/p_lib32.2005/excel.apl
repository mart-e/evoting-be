.head 9 -  Set sAlign = sAlign || SalNumberToStrX( n, 0 )
.head 8 +  Else If pnAttr & XLS_StyleGeneral
.head 9 -  Set sAlign = 'G0'
.head 8 +  If ( pnAttr & ( XLS_Left | XLS_Center | XLS_Right ) ) or ( sAlign != cSLKNull )
.head 9 +  If sAlign = cSLKNull
.head 10 -  Set sAlign = 'G0'
.head 9 +  If pnAttr & XLS_Left
.head 10 -  Set sAlign = sAlign || 'L'
.head 9 +  Else If pnAttr & XLS_Center
.head 10 -  Set sAlign = sAlign || 'C'
.head 9 +  Else If pnAttr & XLS_Right
.head 10 -  Set sAlign = sAlign || 'R'
.head 9 +  Else
.head 10 -  Set sAlign = sAlign || 'G'
.head 8 +  If sAlign != cSLKNull
.head 9 -  Set ls = ls || ';F' || sAlign
.head 7 +  If pnStyle > -1
.head 8 +  If sStyle = cSLKNull
.head 9 -  Set sStyle = 'D'
.head 8 -  Set sStyle = sStyle || 'M' || SalNumberToStrX( pnStyle, 0 )
.head 7 +  If sStyle != cSLKNull
.head 8 -  Set ls = ls || ';S' || sStyle
.head 8 +  If sAlign != cSLKNull
.head 9 -  Set ls = ls || sAlign
.head 7 +  If ls != cSLKNull
.head 8 +  If pnAttr & XLS_Format
