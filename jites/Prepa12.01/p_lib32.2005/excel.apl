.head 9 -  Set n = SalNumberMod( n1, 10 )
.head 9 -  Set n = ( n1 - n ) / 10
.head 9 -  Set ls = ';P' || SalNumberToStrX( n, 0 ) || ls
.head 8 -  Set ls = 'F' || ls || '
'
.head 8 -  Call SalFileWrite( pfh, ls, SalStrLength( ls ) )
