.head 5 +  On VTM_KeyDown
.head 6 -  ! ***** Get selected item
.head 6 -  Set nIdx = VisListGetFocusIndex( hWndItem )
.head 6 +  If (nIdx != LB_Err)
.head 7 +  Select Case wParam
.head 8 -  Case VK_Add       ! *** Expand
