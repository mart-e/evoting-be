.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '%', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '^', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '&', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '*', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '(', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, ')', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '_', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '+', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '-', '' )
.head 6 -  Set psOrig = VisStrSubstitute( psOrig, '=', '' )
