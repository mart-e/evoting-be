.head 9 -  Call SalSetWindowLoc( hWndItem, NewX, NewY )
.head 9 -  Call SalInvalidateWindow( hWndItem )
.head 8 -  ! is this a "resize" control?
.head 8 +  If m_AlignWidth or m_AlignHeight
