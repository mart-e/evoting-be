.head 3 +  Function: PalVerifNNX
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Boolean:
.head 4 +  Parameters
.head 5 -  String: psNN
.head 5 -  Receive String: psNNCalculer
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  Number: lnNN9g
.head 5 -  Boolean: pbOk
.head 5 -  Number: lnNN2d
.head 5 -  Number: lnResultMod97
.head 4 +  Actions
.head 5 -  Set psNN = SalStrRightX('00000000000'||psNN,11)
.head 5 -  Set psNNCalculer = psNN
.head 5 -  Set lnNN9g = SalStrToNumber(SalStrLeftX(psNN,9))
.head 5 -  Set lnNN2d = SalStrToNumber(SalStrRightX(psNN,2))
.head 5 -  Set lnResultMod97 = 97 - SalNumberMod(lnNN9g,97)
.head 5 +  If lnResultMod97 = lnNN2d
.head 6 -  Set pbOk = TRUE
.head 6 -  Return pbOk
.head 5 +  Else
.head 6 -  Set pbOk = FALSE
.head 6 -  Set lnNN9g = SalStrToNumber('2'||SalStrLeftX(psNN,9))
.head 6 -  Set lnResultMod97 = 97 - SalNumberMod(lnNN9g,97)
.head 6 +  If lnResultMod97 = lnNN2d
.head 7 -  Set pbOk = TRUE
.head 7 -  Return pbOk
.head 6 +  Else
.head 7 -  Set pbOk = FALSE
.head 7 -  Return pbOk
