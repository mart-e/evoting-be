.head 7 -  Case 0x4E  ! *** '+' key on numeric keypad
.head 7 +  Case 0x4A  ! *** '-' key on numeric keypad
.head 8 -  Return FALSE
.head 8 -  Break
.head 5 -  !
.head 5 +  On SAM_DoubleClick
.head 6 -  Set nIdx = VisListGetFocusIndex( hWndItem )
