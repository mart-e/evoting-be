.head 7 -  ! ***** Get item flags for selected item
.head 7 +  Loop
.head 8 -  Set nFlags = VisListGetFlags( hWndItem, nIdx )
.head 8 +  If (nFlags != LB_Err)
.head 9 -  Break
.head 8 +  If VisErrorRecovery( CAPTION_ErrorHandler ) = ACTION_Abort
