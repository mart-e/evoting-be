		<Type colID="2" >8</Type>
		<Var colID="3" ></Var>
		<String1 colID="4" >Seuls sont admis à la répartition des sièges :</String1>
		<Separateur colID="5" ></Separateur>
		<String2 colID="6" ></String2>
	</row>
