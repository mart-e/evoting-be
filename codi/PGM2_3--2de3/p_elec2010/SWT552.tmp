.head 5 -  Set gnArrCheckSum[gnCheckSumIndice] = lnData
.head 5 -  Set gnCheckSumIndice = gnCheckSumIndice + 1
.head 5 -  ! Mise à null du suivant !
.head 5 -  Set gnArrCheckSum[gnCheckSumIndice] = NUMBER_Null
