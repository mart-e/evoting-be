.head 3 +  Function: PalSortFormatF
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Boolean:
.head 4 +  Parameters
.head 5 -  String: psFileName
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  File Handle: lhFile
.head 5 -  Boolean: lbPremierG
.head 5 -  Number: lnCount
.head 5 -  Long String: lsLine
.head 5 -  Number: lnMaxLineLength
.head 5 -  Long String: lsLineDebut[*]
.head 5 -  Long String: lsLineListes[*]
.head 5 -  Long String: lsLineCand[*]
.head 5 -  Long String: lsLineFin[*]
.head 5 -  !
.head 5 -  Number: lnNumberOfDebut
.head 5 -  Number: lnNumberOfListes
.head 5 -  Number: lnNumberOfCand
.head 5 -  Number: lnNumberOfFin
.head 5 -  String: lsTri
.head 5 -  String: lsLineWrite
.head 5 -  String: lsOrigFile
.head 5 -  String: lsTmp
.head 4 +  Actions
.head 5 -  ! Initialisation
.head 5 -  Set lnMaxLineLength =  LG_MAX_BUFFER
.head 5 -  !
.head 5 +  If SalStrRightX(  psFileName, 3 ) = 'tmp'
.head 6 -  Set lsOrigFile = SalStrLeftX( psFileName, SalStrLength( psFileName )-1 ) || 'o' 
.head 5 +  Else
.head 6 -  Set lsOrigFile = psFileName || 'o' 
.head 5 +  If NOT ( SalStrUpperX( psFileName ) = SalStrUpperX( lsOrigFile ) )
.head 6 -  Call VisFileDelete( lsOrigFile)
.head 6 -  Call VisFileCopy( psFileName, lsOrigFile)
.head 6 -  Call VisFileDelete( psFileName)
.head 5 -  ! Open file
.head 5 +  If Not SalFileOpen( lhFile, lsOrigFile, OF_Read )
.head 6 -  Call PalMessageBox( 'Impossible de lire le fichier ' || psFileName, 'Erreur 1',
                                  'Het bestand ' || psFileName || ' kan niet gelezen worden', 'Fout 1',
                                  'Datei ' || psFileName || 'kann nicht gelesen werden', 'Fehler',
                                  MB_Ok | MB_IconHand )
.head 6 -  Return FALSE
.head 5 -  ! Lire les lignes
.head 5 +  While SalFileGetStr( lhFile, lsLine, lnMaxLineLength )
.head 6 -  !
.head 6 +  If SalStrMidX( lsLine,1,1) = 'G'
.head 7 -  Set lsLineDebut[lnNumberOfDebut] = lsLine
.head 7 -  Set lnNumberOfDebut = lnNumberOfDebut + 1
.head 6 +  Else If SalStrMidX( lsLine,1,1) = 'S'
.head 7 +  If (SalStrRightX( psFileName,10) ='2P21004.EU' or  SalStrRightX( psFileName,10) ='2P21004.SE'  
or  SalStrRightX( psFileName,10) ='1K21999.SE' )
and SalStrMidX(lsLine,9,5) != gsPar_BureauIns
.head 8 -  Set lsTmp=SalStrMidX(lsLine,8,5) 
.head 7 +  Else
.head 8 -  Set lsTri = SalStrMidX(lsLine,13,1 ) 
.head 8 -  Set lsLineDebut[lnNumberOfDebut] = lsTri || lsLine
.head 8 -  Set lnNumberOfDebut = lnNumberOfDebut + 1
.head 6 +  Else If SalStrMidX( lsLine,1,1) = 'L'
.head 7 +  If (SalStrRightX( psFileName,10) ='2P21004.EU' or  SalStrRightX( psFileName,10) ='2P21004.SE'   
or  SalStrRightX( psFileName,10) ='1K21999.SE' )
and SalStrMidX(lsLine,9,5) != gsPar_BureauIns
.head 8 -  Set lsTmp=SalStrMidX(lsLine,8,5) 
.head 7 +  Else
.head 8 -  ! << 2010
.head 8 +  If SalStrMidX( lsLine,19,1) = '"'
.head 9 -  Set lsTri = SalStrMidX(lsLine,22,3 )
.head 8 +  Else
.head 9 -  Set lsTri = SalStrMidX(lsLine,21,3 )
.head 8 -  ! Set lsTri = SalStrMidX(lsLine,21,3 )
.head 8 -  ! 2010 >>
.head 8 +  If SalStrRightX(lsTri,1) = ','
.head 9 -  ! 2",
.head 9 -  Set lsTri ='0' || SalStrMidX(lsTri,0,1)
.head 9 -  ! 02
.head 8 +  Else If SalStrRightX(lsTri,1) = '"'
.head 9 +  If SalStrLeftX(lsTri,1) = '"'
.head 10 -  ! "2"
.head 10 -  Set lsTri ='0' || SalStrMidX(lsTri,1,1)
.head 10 -  ! 02
.head 9 +  Else
.head 10 -  ! 25"
.head 10 -  Set lsTri =SalStrLeftX(lsTri,2)
.head 10 -  ! 25
.head 8 +  Else
.head 9 -  ! "25
.head 9 -  Set lsTri =SalStrRightX(lsTri,2)
.head 9 -  ! 25
.head 8 -  ! Set lsTri = SalStrMidX(lsLine,13,1 ) ||  lsTri 
.head 8 -  Set lsTri = SalStrMidX(lsLine,13,1 ) || SalStrMidX(lsLine,9,5 )  || lsTri 
.head 8 -  Set lsLineListes[lnNumberOfListes] = lsTri || lsLine
.head 8 -  Set lnNumberOfListes = lnNumberOfListes + 1
.head 6 +  Else If SalStrMidX( lsLine,1,1) = 'C'
.head 7 +  If (SalStrRightX( psFileName,10) ='2P21004.EU' or  SalStrRightX( psFileName,10) ='2P21004.SE'   
or SalStrRightX( psFileName,10) ='1K21999.SE' )
and SalStrMidX(lsLine,9,5) != gsPar_BureauIns
.head 8 -  Set lsTmp=SalStrMidX(lsLine,8,5) 
.head 7 +  Else
.head 8 -  ! << 2010
.head 8 +  If SalStrMidX( lsLine,19,1) = '"'
.head 9 -  Set lsTri = SalStrMidX(lsLine,22,11 )
.head 8 +  Else
.head 9 -  Set lsTri = SalStrMidX(lsLine,21,11 )
.head 8 -  ! Set lsTri = SalStrMidX(lsLine,21,11 )
.head 8 -  ! 2010 >>
.head 8 +  If SalStrMidX(lsTri,1,1) = '"'
.head 9 -  Set lsTri ='0' || lsTri
.head 8 +  If SalStrMidX(lsTri,10,1) = '"'
.head 9 -  Set lsTri = SalStrLeftX(lsTri,9) || '0' || SalStrMidX(lsTri,9,1)
.head 8 -  Set lsTri = SalStrLeftX(lsTri,11)
.head 8 -  !
.head 8 -  ! Set lsTri = SalStrMidX(lsLine,13,1 )  || lsTri 
.head 8 -  Set lsTri = SalStrMidX(lsLine,13,1 ) || SalStrMidX(lsLine,9,5 )  || lsTri 
.head 8 -  !
.head 8 -  Set lsLineCand[lnNumberOfCand] = lsTri || lsLine
.head 8 -  Set lnNumberOfCand = lnNumberOfCand + 1
.head 6 +  Else If SalStrMidX( lsLine,1,1) = 'H'
.head 7 -  Set lsLineFin[lnNumberOfFin] = lsLine
.head 7 -  Set lnNumberOfFin = lnNumberOfFin + 1
.head 6 +  Else If SalStrMidX( lsLine,1,1) = 'T'
.head 7 -  Set lsLineFin[lnNumberOfFin] = lsLine
.head 7 -  Set lnNumberOfFin = lnNumberOfFin + 1
.head 6 -  !
.head 6 -  Set lnCount = lnCount + 1
.head 5 -  ! Clôturer le fichier
.head 5 -  Call SalFileClose( lhFile )
.head 5 -  !
.head 5 -  ! Tri
.head 5 -  Call VisArraySort( lsLineDebut,SORT_Ascending,DT_String )
.head 5 -  Call VisArraySort( lsLineListes,SORT_Ascending,DT_String )
.head 5 -  Call VisArraySort( lsLineCand,SORT_Ascending,DT_String )
.head 5 -  !
.head 5 -  ! Ecriture
.head 5 +  If Not SalFileOpen( lhFile, psFileName , OF_Create | OF_Write )
.head 6 -  Call PalMessageBox( 'Impossible de lire le fichier :  ' || psFileName, 'Erreur 2',
                                  'Het bestand :' || psFileName || ' kan niet gelezen worden', 'Fout',
                                  'Datei :' || psFileName || 'kann nicht gelesen werden', 'Fehler',
                                  MB_Ok | MB_IconHand )
.head 6 -  Return FALSE
.head 5 -  Set lnCount= 0
.head 5 -  Set lbPremierG = TRUE
.head 5 +  While lnCount < lnNumberOfDebut
.head 6 +  If SalStrMidX(lsLineDebut[lnCount],1,3) = '"S"'
.head 7 -  Set lsLineWrite = SalStrMidX(lsLineDebut[lnCount],1, SalStrLength( lsLineDebut[lnCount] )- 1)
.head 7 -  Call SalFilePutStr(lhFile,lsLineWrite)
.head 6 +  Else
.head 7 +  If SalStrMidX(lsLineDebut[lnCount],0,3) = '"G"' AND lbPremierG
.head 8 -  Set lsLineWrite = lsLineDebut[lnCount]
.head 8 -  Set lbPremierG = FALSE
.head 8 -  Call SalFilePutStr(lhFile,lsLineWrite)
.head 6 -  Set lnCount = lnCount + 1
.head 5 -  Set lnCount= 0
.head 5 +  While lnCount < lnNumberOfListes
.head 6 -  Set lsLineWrite = SalStrMidX(lsLineListes[lnCount],8, SalStrLength( lsLineListes[lnCount] )- 8)
.head 6 -  Call SalFilePutStr(lhFile,lsLineWrite)
.head 6 -  Set lnCount = lnCount + 1
.head 5 -  Set lnCount= 0
.head 5 +  While lnCount < lnNumberOfCand
.head 6 -  Set lsLineWrite = SalStrMidX(lsLineCand[lnCount],17, SalStrLength( lsLineCand[lnCount] )- 17)
.head 6 -  Call SalFilePutStr(lhFile,lsLineWrite)
.head 6 -  Set lnCount = lnCount + 1
.head 5 -  Set lnCount= 0
.head 5 +  While lnCount < lnNumberOfFin
.head 6 -  Call SalFilePutStr(lhFile,lsLineFin[lnCount])
.head 6 -  Set lnCount = lnCount + 1
.head 5 -  Call SalFileClose( lhFile )
.head 5 +  If lnNumberOfListes < 1 or lnNumberOfCand  < 1 
.head 6 -  Call PalMessageBox( 'Fichier sans listes ou candidats : ' || psFileName, 'Erreur ',
                                  'Bestand zonder lijst of kandidaten: '  || psFileName, 'Fout',
                                  'Datei  ohne listen oder kandidaten ' || psFileName , 'Fehler',
                                  MB_Ok | MB_IconHand )
.head 6 -  Return FALSE
.head 5 -  Return TRUE
