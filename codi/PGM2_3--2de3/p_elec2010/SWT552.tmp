.head 5 +  Function: PalBuid0kFileFrom1K
.head 6 -  Description: 
Création d'un fichier résultats provisoires (0k) au départ d'un fichier 1k.
Fichier 0k = fichier 1k sans les enregistrements de type 'C'
.head 6 +  Returns
.head 7 -  Boolean:
.head 6 +  Parameters
.head 7 -  String: psPathnameFichier1K
.head 7 -  String: psPathnameFichier0K
.head 7 -  String: psFilenameFichier0K
.head 6 -  Static Variables
.head 6 +  Local variables
.head 7 -  File Handle: lhFile1k
.head 7 -  File Handle: lhFile0k
.head 7 -  String: lsFileLine
.head 7 -  Number: lnMaxLineLength
.head 7 -  String: lsTypeEnreg
.head 7 -  !
.head 7 -  Number: lnLineLength
.head 7 -  Number: lnFileSize
.head 7 -  !
.head 7 -  String: lsEnregH
.head 7 -  String: lsEnregHFileSize
.head 7 -  String: lsEnregT
.head 7 -  ! lsEnregT_GV:
 G(partiel),V(complet)
.head 7 -  String: lsEnregT_GV
.head 7 -  String: lsTailleDuFichier
.head 6 +  Actions
.head 7 -  ! Call SalMessageBox( psFichier0K, psFichier1K, 0 )
.head 7 -  !
.head 7 -  ! Ouverture du fichier 1K en lecture
.head 7 +  If NOT SalFileOpen( lhFile1k, psPathnameFichier1K, OF_Read )
.head 8 -  Call SalMessageBox( 'Operation canceled
Can\'t open file "' || psPathnameFichier1K || '"', 'Error', 0 )
.head 8 -  Return FALSE
.head 7 -  !
.head 7 -  ! Ouverture du fichier 1K en écriture
.head 7 +  If NOT SalFileOpen( lhFile0k, psPathnameFichier0K, OF_Create | OF_ReadWrite )
.head 8 -  Call SalMessageBox( 'Operation canceled
Can\'t create file "' || psPathnameFichier0K || '"', 'Error', 0 )
.head 8 -  Call SalFileClose( lhFile1k )
.head 8 -  Return FALSE
.head 7 -  !
.head 7 -  ! On recopie dans le fichier 0K l'ensemble des lignes de 1K ne correspondant pas à un enregistrement de type candidat 'C'
.head 7 -  Set lnFileSize = 0
.head 7 -  !
.head 7 -  ! Enregistrement de type H
.head 7 -  Set lsEnregHFileSize = 'FILESIZE'
.head 7 -  !
.head 7 -  ! Enregistrement de type T : G (partiel), V (complet)
.head 7 +  If gUDVFichierArr.iv_nNumberOfRecordsTotPar = gUDVFichierArr.iv_nNumberOfRecordsDetail
.head 8 -  Set lsEnregT_GV = 'V'
.head 7 +  Else
.head 8 -  Set lsEnregT_GV = 'G'
.head 7 -  !
.head 7 -  Set lnMaxLineLength =  LG_MAX_BUFFER
.head 7 +  While SalFileGetStr( lhFile1k, lsFileLine, lnMaxLineLength )
.head 8 -  Set lsTypeEnreg = SalStrLeftX( lsFileLine, 3 )
.head 8 +  If lsTypeEnreg  = '"H"' 
.head 9 -  Set lsEnregH = '"H"' || ',"' || psFilenameFichier0K || '","' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd/MM/yyyy' ) || '","' 
.head 9 -  Set lsEnregH = lsEnregH || SalFmtFormatDateTime( SalDateCurrent( ), 'hhhh:mm:ss' ) || '","' || lsEnregHFileSize || '","' || gUDV_Param1Cfg.Param1_GetBureauNom2_FF(  ) || '",""' 
.head 9 -  ! lnFileSize
.head 9 -  Set lnFileSize = lnFileSize + SalStrLength( lsEnregH ) - SalStrLength( lsEnregHFileSize ) +1
.head 8 +  Else If lsTypeEnreg  = '"T"' 
.head 9 -  Set lsEnregT = '"T"' || ',"'  || lsEnregT_GV || '","0","' || SalNumberToStrX(gUDVFichierArr.iv_nNumberOfRecordsTotPar, 0) || '","' || SalNumberToStrX(gUDVFichierArr.iv_nNumberOfRecordsDetail, 0) || '","' 
.head 9 -  Set lsEnregT = lsEnregT || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd/MM/yyyy' ) || '","' || SalFmtFormatDateTime( SalDateCurrent( ), 'hhhh:mm:ss' ) || '"'
.head 9 -  ! lnLineLength
.head 9 -  Set lnFileSize = lnFileSize + SalStrLength( lsEnregT ) +1
.head 8 +  Else If NOT lsTypeEnreg  = '"C"'
.head 9 -  ! lnLineLength
.head 9 -  Set lnLineLength = SalStrLength( lsFileLine )  +1
.head 9 -  !
.head 9 -  ! << 200e
.head 9 -  ! Call SalFileWrite( lhFile0k, lsFileLine || '
', lnLineLength )
.head 9 -  Call SalFilePutStr( lhFile0k, lsFileLine )
.head 9 -  ! 200e >>
.head 9 -  Set lnFileSize = lnFileSize + lnLineLength
.head 7 -  !
.head 7 -  ! Taille du fichier en octets
.head 7 -  Set lsTailleDuFichier = SalNumberToStrX( lnFileSize, 0 )
.head 7 -  Set lnFileSize = lnFileSize + SalStrLength (SalNumberToStrX( lnFileSize + SalStrLength( lsTailleDuFichier ), 0 ) )
.head 7 -  Set lsTailleDuFichier = SalNumberToStrX( lnFileSize, 0 )
.head 7 -  Set lsEnregH = VisStrSubstitute( lsEnregH, lsEnregHFileSize, lsTailleDuFichier )
.head 7 -  !
.head 7 -  ! H
.head 7 -  Call SalFilePutStr( lhFile0k, lsEnregH )
.head 7 -  ! T
.head 7 -  Call SalFilePutStr( lhFile0k, lsEnregT )
.head 7 -  !
.head 7 -  ! Call SalMessageBox( SalNumberToStrX( lnFileSize, 0 ), '', 0 )
.head 7 -  ! Fermeture  des fichiers
.head 7 -  Call SalFileClose( lhFile1k )
.head 7 -  Call SalFileClose( lhFile0k )
.head 7 -  Return TRUE
