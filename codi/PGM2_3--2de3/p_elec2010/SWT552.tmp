.head 8 -  Call SalArrayGetUpperBound( iv_nArrGroupBox_hWnd, 1, lnIndex )
.head 8 -  Call SalArraySetUpperBound( iv_nArrGroupBox_hWnd, 1, lnIndex + 1 )
.head 8 -  Set iv_nArrGroupBox_hWnd[lnIndex] = lnhWndGbNumber
