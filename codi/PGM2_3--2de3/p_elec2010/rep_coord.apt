.head 7 -                                                                                                                                                                                                                                                                                                                     Set lbContinue = TRUE
.head 7 +                                                                                                                                                                                                                                                                                                                     If Not SalFileOpen( lhFile, psFileName_Jites, OF_Read )
.head 8 -                                                                                                                                                                                                                                                                                                                     Call PalMessageBox( 'Impossible d\'ouvrir le fichier ' || psFileName_Jites,
			'Het bestand ' || psFileName_Jites || ' kan niet geopened worden',
			'Datei kann nicht geöffnet werden'  || psFileName_Jites,
			 'Erreur', 'Fout', 'Fehler', MB_Ok | MB_IconHand)
.head 8 -                                                                                                                                                                                                                                                                                                                     Return FALSE
.head 7 +                                                                                                                                                                                                                                                                                                                     While lbContinue And SalFileGetStr( lhFile, lsLine, 512 )
.head 8 +                                                                                                                                                                                                                                                                                                                     If Not ( SalStrLength( lsLine ) = 69 Or SalStrLength( lsLine ) = 57 Or SalStrLength( lsLine ) = 36 Or SalStrLength( lsLine ) = 14 )
.head 9 -                                                                                                                                                                                                                                                                                                                     Call PalMessageBox( 'Le fichier '  || psFileName_Jites || ' n\'a pas le format JITES',
			'Het bestand ' || psFileName_Jites || ' is niet in het formaat JITES',
			'pas le format Jites ' || psFileName_Jites || ' pas le format  JITES DE',
			'Erreur', 'Fout', 'Fehler', MB_Ok | MB_IconHand)
.head 9 -                                                                                                                                                                                                                                                                                                                     Set lbContinue = FALSE
.head 9 -                                                                                                                                                                                                                                                                                                                     Return FALSE
.head 8 -                                                                                                                                                                                                                                                                                                                     Set lbContinue = FALSE
.head 7 -                                                                                                                                                                                                                                                                                                                     Call SalFileClose( lhFile )
