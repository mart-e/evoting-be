		'Numéro de candidat de' || SalNumberToStrX( iv_nCandListeOrdreNo_Jites, 0 ) || ' est inconnu dans le fichier JITES DE' || psFileName_Jites,
		'Erreur', 'Fout', 'Fehler', MB_Ok | MB_IconHand  )
.head 10 -                                                                                                                                                                                                                                                                                                                     Return FALSE
.head 9 -                                                                                                                                                                                                                                                                                                                     Call pUDVFichierArr.SetNbreVoixObtenus( psFileName_Elec, lnIndexCand, iv_nNbreVoixObtenus_Jites )
.head 7 -                                                                                                                                                                                                                                                                                                                     Call SalFileClose( lhFile )
