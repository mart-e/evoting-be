.head 6 +                                                                                                                                                                                                                                                                                                                     Actions 
.head 7 +                                                                                                                                                                                                                                                                                                                     Select Case pnElectionType_Bull
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 1
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'BR'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 2
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = '??'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 3
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'CK'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 4
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'SE'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 5
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'DE'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 6
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = '??'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 7
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'EU'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 8
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'VL'
.head 8 +                                                                                                                                                                                                                                                                                                                     Case 9
.head 9 -                                                                                                                                                                                                                                                                                                                     Return psElectionType_Elec = 'WL'
