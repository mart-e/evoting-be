.head 3 +  Function: PalCheckSum
.head 4 -  Description:
.head 4 +  Returns
.head 5 -  Number:
.head 4 +  Parameters
.head 5 -  Number: pnArrData[*]
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  Number: lnArrData[*]
.head 5 -  Number: lni
.head 4 +  Actions
.head 5 -  Set lni = 0
.head 5 +  While pnArrData[lni] != NUMBER_Null
.head 6 +  If lni = 0
.head 7 -  Set lnArrData[lni]= pnArrData[lni]
.head 6 +  Else
.head 7 +  If pnArrData[lni] = 0
.head 8 -  ! si 0 alors nouveau = précedent + 9
.head 8 -  Set lnArrData[lni] = lnArrData[lni-1]+ 9
.head 7 +  Else
.head 8 -  ! si <> 0 alors nouveau = modulo 99997 de (( précédent * nouveau) + 1 )
.head 8 -  Set lnArrData[lni] = SalNumberMod( ( lnArrData[lni-1] * pnArrData[lni] ) + 1,99997)
.head 7 -  !
.head 6 -  Set lni = lni +1
.head 5 -  Return lnArrData[lni - 1]
.head 3 +  Function: PalAddCheckSum
.head 4 -  Description:
.head 4 -  Returns
.head 4 +  Parameters
