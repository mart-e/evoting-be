.head 10 -                                                                                                                                                                                                                                                                                                                     Call pUDVFichierArr.SetNbreBulletinsValables( psFileName_Elec, lnIndexScrutin_NN, iv_nBulletinsValablesNN_Jites )
.head 10 -                                                                                                                                                                                                                                                                                                                     Call pUDVFichierArr.SetNbreBulletinsNulBlancs( psFileName_Elec, lnIndexScrutin_FF, iv_nBulletinsNulBlancs_Jites )
