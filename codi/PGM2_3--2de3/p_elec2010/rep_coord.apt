.head 8 +                                                                                                                                                                                                                                                                                                                     If SalStrLength( lsLine ) > 50 And __ConvertElectionType( iv_sElectionType_Jites, pUDVFichierArr.GetElectionType( psFileName_Elec, 0 ) )
.head 9 +                                                                                                                                                                                                                                                                                                                     If iv_nBulletinsValablesNN_Jites > 0 Or iv_nBulletinsValablesFF_Jites > 0
.head 10 -                                                                                                                                                                                                                                                                                                                     Set iv_nNumberOfScrutins_Jites = 2
.head 9 +                                                                                                                                                                                                                                                                                                                     Else 
.head 10 -                                                                                                                                                                                                                                                                                                                     Set iv_nNumberOfScrutins_Jites = 1
